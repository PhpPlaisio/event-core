    "ABC-Framework",
    "Core",
    "Event"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "setbased/abc": "*",
    "setbased/abc-console": "*",
    "setbased/abc-event": "*",
    "setbased/abc-exception": "^2.0.0",
    "setbased/helper-code-store-php": "^2.1.1",
    "symfony/console": "^4.0.0"
  },
  "require-dev": {
