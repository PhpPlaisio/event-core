{
  "name": "plaisio/event-core",
  "description": "PhpPlaisio: Core Event",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Event"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/kernel": "^1.0",
    "plaisio/console": "^0.2.1",
    "plaisio/event": "^1.0",
    "plaisio/exception": "^1.0",
    "setbased/helper-code-store-php": "^2.3",
    "symfony/console": "^5.0"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Event\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Event\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/"
  }
}
