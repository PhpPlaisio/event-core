{
  "name": "plaisio/event-core",
  "description": "PhpPlaisio: Core Event",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Event"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-dom": "*",
    "ext-libxml": "*",
    "plaisio/console": "^3.1.1",
    "plaisio/event": "^3.0.0",
    "plaisio/exception": "^1.2.0",
    "plaisio/kernel": "^3.2.1",
    "setbased/helper-code-store-php": "^2.7.0",
    "symfony/console": "^6.0|^7.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "plaisio/company-resolver-uni": "^2.1.1",
    "plaisio/console-kernel": "^1.1.0",
    "setbased/error-handler": "1.3.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Event\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Event\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
